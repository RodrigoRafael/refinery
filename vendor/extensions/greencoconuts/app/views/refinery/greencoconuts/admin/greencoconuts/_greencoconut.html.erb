<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(greencoconut) -%>">
  <span class='title'>
    <%= greencoconut.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.greencoconuts_greencoconut_path(greencoconut),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_greencoconuts_admin_greencoconut_path(greencoconut),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.greencoconuts_admin_greencoconut_path(greencoconut),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => greencoconut.name),
        :method => :delete %>
  </span>
</li>
